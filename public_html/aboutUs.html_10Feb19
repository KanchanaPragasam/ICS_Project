    <script src="js/custom.js"></script><script>	$(document).ready(function(){	
console.log('aboutus');
console.log(localStorage.getItem('NG_TRANSLATE_LANG_KEY'));
if (localStorage.getItem('NG_TRANSLATE_LANG_KEY') == 'en'){
$('#eng').addClass('active');
} else {
$('#frn').addClass('active');
}
	$('#mailform').submit(function(event) {		$('#response').html("<b style='color: white; '>{{'Loading response...' | translate}}</b>");				$.ajax({		type: 'POST',		url: 'mail.php',		data: $(this).serialize() 		})		.done(function(data){ 			$('#response').html(data);			$("#mailform")[0].reset();		})		.fail(function() {			console.log( "{{'Posting failed.' | translate}}" );		});				return false;		});	});</script>

    <script>
        $(function () {
            $('.carousel').carousel({
                interval: 5000
            });						 var options = {                useEasing: true,                useGrouping: true,                separator: '',                decimal: '.',                suffix: '+'            };					var member = new CountUp('member', 0, 20, 0, 4, options);            var projects = new CountUp('projects', 0, 25, 0, 4, options);            var customers = new CountUp('customers', 0, 5, 0, 4, options);            var experience = new CountUp('experience', 0, 10, 0, 4, options);            var aniCount = 0;            $(window).scroll(function () {                countAnimation();            });            function countAnimation() {                if ($('#member').hasClass('animated') && (aniCount == 0)) {                    aniCount++;                    setTimeout(() => {                        member.start();                    }, 200);                    setTimeout(() => {                        projects.start();                    }, 500);                    setTimeout(() => {                        customers.start();                    }, 750);                    setTimeout(() => {                        experience.start();                    }, 1000);                }            }            setTimeout(() => {                countAnimation();            }, 1000);			
        });

        new WOW().init();

    </script>
